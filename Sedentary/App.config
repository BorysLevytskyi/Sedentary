<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
 <system.diagnostics>
    <trace autoflush="false" indentsize="4">
      <listeners>
        <add name="myListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="TextWriterOutput.log" />
      </listeners>
    </trace>
  </system.diagnostics>
  <appSettings>
    <add key="awayThreshold" value="00:05"/>
    <add key="maxSittingTime" value="01:00"/>
    <add key="requiredRestingTime" value="00:05"/>
  </appSettings>
</configuration>